<?xml version="1.0"?>
<configuration>

	<appSettings>

        <!-- 
        
        LTN = LUTON
        PFO = PAPHOS
        SSH = SHARM EL SHEIKH            
        
        -->
        
		<add key="url" value="http://www.easyjet.com/EN/linkedAirportsJSON"/>
        <add key="urltobrowseonchange" value="http://www.easyjet.com"/>
		<add key="useragent" value="Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.0; Trident/5.0)"/>
		<add key="dateregex" value="LTN\|PFO\|\d{4}-\d{2}-\d{2}\|(?&lt;enddate&gt;\d{4}-\d{2}-\d{2})"/>
		<add key="matchgroupname" value="enddate"/>
		<add key="pollintervalminutes" value="10"/>

	</appSettings>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
	</startup>
</configuration>
